package com.mj;

import com.mj.tools.Asserts;
import com.mj.union.UnionFind;
import com.mj.union.UnionFind_QF;
import com.mj.union.UnionFind_QU;

public class Main {
	public static void main(String[] args) {
		UnionFind uf = new UnionFind_QU(16);
		uf.union(0, 1);
		uf.union(0, 3);
		uf.union(0, 4);
		uf.union(2, 3);
		uf.union(2, 5);
		
		uf.union(6, 7);
		
		uf.union(9, 8);
		uf.union(9, 10);
		uf.union(9, 11);
		
		Asserts.test(!uf.isSame(0, 7));
		uf.union(1, 6);
		Asserts.test(uf.isSame(0, 7));
		
	}
}
