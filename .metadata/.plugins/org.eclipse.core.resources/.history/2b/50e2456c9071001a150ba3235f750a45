package com.mj.union;

import java.util.HashMap;
import java.util.Map;

public class GenericUnionFind<V> {
	
	private Map<V, Node<V>> nodes = new HashMap<>();
	
	
	
	
	
	
	
	private static class Node<V> {
		V value;
		Node<V> parent = this;
		int rank = 1;
		Node(V value) {
			this.value = value;
		}
	}
	
}
